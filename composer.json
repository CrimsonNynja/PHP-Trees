{
    "name": "crimson-nynja/php-trees",
    "description": "PHP implementation of various Trees",
    "license": "GPL-3.0-or-later",
    "keywords": ["PHP", "Trees", "Tree", "Binary Search Tree", "BST", "Rope", "Binary Heap", "Heap", "Generic Tree", "PHP 8"],
    "authors": [
        {
            "name": "Hudson Cassidy",
            "email": "hudoc96@hotmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0"
    },
    "autoload": {
        "psr-4": {
            "PhpTrees\\": "src/PhpTrees/"
        },
        "files": [
            "src/PhpTrees/RopeFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.29"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always --bootstrap vendor/autoload.php tests",
        "coverage": "./vendor/bin/phpunit -c phpunit.coverage.xml --do-not-cache-result"
    }
}
